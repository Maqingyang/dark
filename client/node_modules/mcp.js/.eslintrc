{
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 8
	},
	"env":{
		"node":true,
		"es6":true,
		"browser": true
	},
	"rules": {
			"semi": ["error", "always"],
			"quotes": "off",
			"no-console":"off",
			"no-unused-vars":"off",
			"no-unreachable":"off",
			"no-redeclare":"warn",
			"prefer-const":"error",
			"no-case-declarations":"off"
	}
}